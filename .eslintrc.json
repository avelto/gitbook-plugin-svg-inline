{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "extends": ["eslint:recommended"],
  "plugins": [],
  "rules": {
    "no-console": "off",
    "quotes": ["error", "single", "avoid-escape"],
    "space-in-parens": ["error", "always"],
    "object-curly-spacing": ["error", "always"]
  },
  "globals": {
    "window": true,
    "document": true,
    "module": true
  }
}
